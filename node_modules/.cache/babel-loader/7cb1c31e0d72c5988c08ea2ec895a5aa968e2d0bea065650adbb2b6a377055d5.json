{"ast":null,"code":"import moment from 'moment';\nexport default {\n  data() {\n    return {\n      activeName: 'first',\n      value_barang_masuk: null,\n      value_barang_keluar: null,\n      id_kelola_barang_masuk: null,\n      id_kelola_barang_keluar: null\n    };\n  },\n  filters: {\n    tanggal: function (value) {\n      moment.locale('id');\n      return moment(value).format('dddd') + ', ' + moment(value).format('LL');\n    },\n    waktu: function (value) {\n      moment.locale('id');\n      return moment(value).format('hh:mm');\n    }\n  },\n  computed: {\n    get_kelolas() {\n      return this.$store.state.kelola.kelola;\n    },\n    get_masuks() {\n      if (this.id_kelola_barang_masuk != null) {\n        return this.$store.state.masuk.masuk.filter(text => {\n          if (!text.kelolaId) text.kelolaId = '';\n          return text.kelolaId.includes(this.id_kelola_barang_masuk);\n        });\n      }\n      return this.$store.state.masuk.masuk;\n      // return null;\n    },\n\n    get_keluars() {\n      return this.$store.state.keluar.keluar;\n    },\n    get_loading_1() {\n      return this.$store.state.kelola.loading;\n    },\n    get_loading_2() {\n      return this.$store.state.masuk.loading;\n    },\n    get_loading_3() {\n      return this.$store.state.keluar.loading;\n    }\n  },\n  created() {\n    this.$store.dispatch('kelola/get_kelolas');\n    this.$store.dispatch('masuk/get_masuks');\n    this.$store.dispatch('keluar/get_keluars');\n  },\n  methods: {\n    search_barang_masuk: function () {\n      this.value_barang_masuk = this.id_kelola_barang_masuk;\n      console.log(this.value_barang_masuk);\n      console.log(this.id_kelola_barang_masuk);\n    },\n    refresh_barang_masuk: function () {\n      this.value_barang_masuk = null;\n      this.id_kelola_barang_masuk = null;\n    }\n  }\n};","map":{"version":3,"mappings":"AA+IA;AACA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACAC;MACA;IACA;IACAC;MACAD;MACA;IACA;EACA;EACAE;IACAC;MACA;IACA;IACAC;MACA;QACA;UACA;UACA;QACA;MACA;MAEA;MACA;IACA;;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACA;IACA;IACA;EACA;EACAC;IACAC;MACA;MACAC;MACAA;IACA;IACAC;MACA;MACA;IACA;EACA;AACA","names":["data","activeName","value_barang_masuk","value_barang_keluar","id_kelola_barang_masuk","id_kelola_barang_keluar","filters","tanggal","moment","waktu","computed","get_kelolas","get_masuks","get_keluars","get_loading_1","get_loading_2","get_loading_3","created","methods","search_barang_masuk","console","refresh_barang_masuk"],"sourceRoot":"src/views/adm","sources":["LaporanView.vue"],"sourcesContent":["<template>\n  <div>\n    <el-breadcrumb separator=\"/\" class=\"font-4 mb-3\">\n      <el-breadcrumb-item :to=\"{ path: '/dashboard' }\">Dashboard</el-breadcrumb-item>\n      <el-breadcrumb-item>Laporan</el-breadcrumb-item>\n    </el-breadcrumb>\n    <el-tabs v-model=\"activeName\" type=\"border-card\">\n      <el-tab-pane class=\"font-4\" name=\"first\" label=\"Laporan Barang\">\n        <div>\n          <div v-if=\"get_loading_1\">\n            <i class=\"el-icon-loading\"></i>\n          </div>\n          <div v-if=\"!get_loading_1\">\n            {{ get_kelolas }}\n            <div>\n              <div class=\"flex justify-between items-center py-1 border-b\">\n                <div class=\"w-64\">\n                  <p class=\"font-4 text-sm\">Kode barang</p>\n                </div>\n                <div class=\"w-1/2\">\n                  <p class=\"font-64 text-sm\">Nama barang</p>\n                </div>\n                <div class=\"flex-grow\">\n                  <p class=\"font-4 text-sm\">Stok</p>\n                </div>\n              </div>\n            </div>\n            <div v-for=\"(item, index) in get_kelolas\" :key=\"index\">\n              <div class=\"flex justify-between items-center py-2 hover:bg-gray-50 cursor-pointer border-b\">\n                <div class=\"w-64\">\n                  <p class=\"font-4\" v-if=\"item.barangId == null\">Tidak kode barang</p>\n                  <p class=\"font-4 uppercase\" v-if=\"item.barangId != null\">{{ item.barang.kode_barang }}</p>\n                </div>\n                <div class=\"w-1/2\">\n                  <p class=\"font-64\" v-if=\"item.barangId == null\">Tidak nama barang</p>\n                  <p class=\"font-64\" v-if=\"item.barangId != null\">{{ item.barang.nama_barang }}</p>\n                </div>\n                <div class=\"flex-grow\">\n                  <p class=\"font-4\" v-if=\"item.stok == null\">0</p>\n                  <p class=\"font-4\" v-if=\"item.stok != null\">{{ item.stok }}</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </el-tab-pane>\n      <el-tab-pane class=\"font-4\" name=\"second\" label=\"Laporan Barang Masuk\">\n        <div>\n          <div v-if=\"get_loading_2\">\n            <i class=\"el-icon-loading\"></i>\n          </div>\n          <div v-if=\"!get_loading_2\">\n            {{  get_masuks  }}\n            <div>\n              <div class=\"flex justify-end items-center\">\n                <div>\n                  <el-select v-model=\"value_barang_masuk\" filterable placeholder=\"Select\" size=\"mini\">\n                    <el-option v-for=\"item in get_kelolas\" :key=\"item.barang.id\" :label=\"item.barang.nama_barang\" :value=\"item.id\"></el-option>\n                  </el-select>\n                </div>\n                <div class=\"ml-0.5\">\n                  <el-button icon=\"el-icon-search\" size=\"mini\" @click=\"search_barang_masuk\" circle></el-button>\n                </div>\n                <div class=\"ml-0.5\">\n                  <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"refresh_barang_masuk\" circle></el-button>\n                </div>\n              </div>\n            </div>\n            <div>\n              <div class=\"flex justify-between items-center py-1 border-b\">\n                <div class=\"w-1/2\">\n                  <p class=\"font-64 text-xs\">Hari, Tanggal</p>\n                </div>\n                <div class=\"flex-grow\">\n                  <p class=\"font-4 text-xs\">Stok</p>\n                </div>\n              </div>\n            </div>\n            <div v-for=\"(item, index) in get_masuks\" :key=\"index\">\n              <div class=\"flex justify-between items-center py-2 hover:bg-gray-50 cursor-pointer border-b\">\n                <div class=\"w-1/2\">\n                  <p class=\"font-64 text-sm\" v-if=\"item.createdAt != null\">{{ item.createdAt | tanggal }} <span class=\"ml-2\"><i class=\"el-icon-time\"></i> {{ item.createdAt | waktu }}</span></p>\n                </div>\n                <div class=\"flex-grow\">\n                  <p class=\"font-4 text-sm\" v-if=\"item.stok == null\">0</p>\n                  <p class=\"font-4 text-sm\" v-if=\"item.stok != null\">{{ item.stok }}</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </el-tab-pane>\n      <el-tab-pane class=\"font-4\" name=\"thirth\" label=\"Laporan Barang Keluar\">\n        <div>\n          <div v-if=\"get_loading_3\">\n            <i class=\"el-icon-loading\"></i>\n          </div>\n          <div v-if=\"!get_loading_3\">\n            <!-- {{  get_keluars  }} -->\n            <div>\n              <div class=\"flex justify-end items-center\">\n                <div>\n                  <el-select v-model=\"value_barang_keluar\" filterable placeholder=\"Select\" size=\"mini\">\n                    <el-option v-for=\"item in get_kelolas\" :key=\"item.id\" :label=\"item.barang.nama_barang\" :value=\"item.id\"></el-option>\n                  </el-select>\n                </div>\n                <div class=\"ml-0.5\">\n                  <el-button icon=\"el-icon-search\" size=\"mini\" circle></el-button>\n                </div>\n                <div class=\"ml-0.5\">\n                  <el-button icon=\"el-icon-refresh\" size=\"mini\" circle></el-button>\n                </div>\n              </div>\n            </div>\n            <div>\n              <div class=\"flex justify-between items-center py-1 border-b\">\n                <div class=\"w-1/2\">\n                  <p class=\"font-64 text-xs\">Hari, Tanggal</p>\n                </div>\n                <div class=\"flex-grow\">\n                  <p class=\"font-4 text-xs\">Stok</p>\n                </div>\n              </div>\n            </div>\n            <div v-for=\"(item, index) in get_keluars\" :key=\"index\">\n              <div class=\"flex justify-between items-center py-2 hover:bg-gray-50 cursor-pointer border-b\">\n                <div class=\"w-1/2\">\n                  <p class=\"font-64 text-sm\" v-if=\"item.createdAt != null\">{{ item.createdAt | tanggal }} <span class=\"ml-2\"><i class=\"el-icon-time\"></i> {{ item.createdAt | waktu }}</span></p>\n                </div>\n                <div class=\"flex-grow\">\n                  <p class=\"font-4 text-sm\" v-if=\"item.stok == null\">0</p>\n                  <p class=\"font-4 text-sm\" v-if=\"item.stok != null\">{{ item.stok }}</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </el-tab-pane>\n      <el-tab-pane class=\"font-4\" name=\"forth\" label=\"History Barang\">History Barang</el-tab-pane>\n    </el-tabs>\n  </div>\n</template>\n<script>\nimport moment from 'moment'\nexport default {\n  data() {\n    return {\n      activeName: 'first',\n      value_barang_masuk: null,\n      value_barang_keluar: null,\n      id_kelola_barang_masuk: null,\n      id_kelola_barang_keluar: null\n    }\n  },\n  filters: {\n    tanggal: function(value) {\n      moment.locale('id');\n      return moment(value).format('dddd')+', '+moment(value).format('LL');\n    },\n    waktu: function(value) {\n      moment.locale('id');\n      return moment(value).format('hh:mm');\n    }\n  },\n  computed: {\n    get_kelolas() {\n      return this.$store.state.kelola.kelola\n    },\n    get_masuks() {\n      if (this.id_kelola_barang_masuk != null) {\n        return this.$store.state.masuk.masuk.filter((text) => {\n          if (!text.kelolaId) text.kelolaId = ''\n          return text.kelolaId.includes(this.id_kelola_barang_masuk)\n        })\n      }\n\n      return this.$store.state.masuk.masuk\n      // return null;\n    },\n    get_keluars() {\n      return this.$store.state.keluar.keluar\n    },\n    get_loading_1() {\n      return this.$store.state.kelola.loading\n    },\n    get_loading_2() {\n      return this.$store.state.masuk.loading\n    },\n    get_loading_3() {\n      return this.$store.state.keluar.loading\n    },\n  },\n  created() {\n    this.$store.dispatch('kelola/get_kelolas')\n    this.$store.dispatch('masuk/get_masuks')\n    this.$store.dispatch('keluar/get_keluars')\n  },\n  methods: {\n    search_barang_masuk: function(){\n      this.value_barang_masuk = this.id_kelola_barang_masuk;\n      console.log(this.value_barang_masuk);\n      console.log(this.id_kelola_barang_masuk);\n    },\n    refresh_barang_masuk: function(){\n      this.value_barang_masuk = null;\n      this.id_kelola_barang_masuk = null;\n    }\n  },\n}\n</script>\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=Ubuntu:wght@500&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Dosis:wght@200&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Quicksand:wght@700&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&display=swap');\n.font-1{font-family: 'Ubuntu', sans-serif;}\n.font-2{font-family: 'Dosis', sans-serif;}\n.font-3{font-family: 'Quicksand', sans-serif;}\n.font-4{font-family: 'Open Sans', sans-serif;}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}