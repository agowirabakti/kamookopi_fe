{"ast":null,"code":"export default {\n  data() {\n    return {\n      activeName: 'first',\n      dialogvisible: false\n    };\n  },\n  computed: {\n    get_users() {\n      return this.$store.state.pengguna.user;\n    },\n    get_loading() {\n      return this.$store.state.pengguna.loading;\n    }\n  },\n  created() {\n    this.$store.dispatch('pengguna/get_users');\n  },\n  methods: {\n    open_dialogvisible: function () {\n      this.dialogvisible = true;\n    },\n    close_dialogvisible: function () {\n      this.dialogvisible = false;\n    },\n    submit_dialogvisible: function () {\n      this.dialogvisible = false;\n    },\n    cancel_dialogvisible: function () {\n      this.dialogvisible = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAgEA;EACAA;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["data","activeName","dialogvisible","computed","get_users","get_loading","created","methods","open_dialogvisible","close_dialogvisible","submit_dialogvisible","cancel_dialogvisible"],"sourceRoot":"src/views/adm","sources":["DatamasterView.vue"],"sourcesContent":["<template>\n  <div>\n    <el-breadcrumb separator=\"/\" class=\"font-4 mb-3\">\n      <el-breadcrumb-item :to=\"{ path: '/dashboard' }\">Dashboard</el-breadcrumb-item>\n      <el-breadcrumb-item>Data Master</el-breadcrumb-item>\n    </el-breadcrumb>\n    <el-tabs v-model=\"activeName\" class=\"font-3\" type=\"border-card\">\n      <el-tab-pane class=\"font-4\" name=\"first\" label=\"Data Barang\">\n        <div>\n          <div v-if=\"get_loading\">\n            <i class=\"el-icon-loading\"></i>\n          </div>\n          <div v-if=\"!get_loading\">\n            ...\n          </div>\n        </div>\n      </el-tab-pane>\n      <el-tab-pane class=\"font-4\" name=\"second\" label=\"Data User\">\n        <div>\n          <div v-if=\"get_loading\">\n            <i class=\"el-icon-loading\"></i>\n          </div>\n          <div v-if=\"!get_loading\">\n            <div class=\"flex justify-end mb-2\">\n              <div>\n                <el-button type=\"primary\" size=\"mini\"><i class=\"el-icon-user\"></i> Tambah User</el-button>\n              </div>\n            </div>\n            <div v-for=\"(item, index) in get_users\" :key=\"index\">\n              <div class=\"flex justify-between items-center py-2 hover:bg-gray-50 cursor-pointer border-b\">\n                <div class=\"flex-grow w-24\">\n                  <p class=\"font-4\" v-if=\"item.name == null\">No name</p>\n                  <p class=\"font-4\" v-if=\"item.name != null\">{{ item.name }}</p>\n                </div>\n                <div class=\"flex-grow w-24\">\n                  <p class=\"font-4\" v-if=\"item.username == null\">No username</p>\n                  <p class=\"font-4\" v-if=\"item.username != null\">{{ item.username }}</p>\n                </div>\n                <div class=\"flex-grow w-24\">\n                  <p class=\"font-4 text-sm\" v-if=\"item.status == null\">No status</p>\n                  <p class=\"font-4 text-sm\" v-if=\"item.status != null\">{{ item.status }}</p>\n                </div>\n                <div class=\"ml-0.5\">\n                  <el-button size=\"mini\" circle><i class=\"el-icon-edit\"></i></el-button>\n                </div>\n                <div class=\"ml-0.5\">\n                  <el-button size=\"mini\" circle><i class=\"el-icon-delete\"></i></el-button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </el-tab-pane>\n    </el-tabs>\n    <el-dialog title=\"Tips\" :visible.sync=\"dialogvisible\" width=\"30%\" :before-close=\"close_dialogvisible\">\n      <span>This is a message</span>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"cancel_dialogvisible\" size=\"mini\">Cancel</el-button>\n        <el-button type=\"primary\" @click=\"submit_dialogvisible\" size=\"mini\">Submit</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      activeName: 'first',\n      dialogvisible: false\n    }\n  },\n  computed: {\n    get_users() {\n      return this.$store.state.pengguna.user\n    },\n    get_loading() {\n      return this.$store.state.pengguna.loading\n    }\n  },\n  created() {\n    this.$store.dispatch('pengguna/get_users')\n  },\n  methods: {\n    open_dialogvisible: function() {\n      this.dialogvisible = true;\n    },\n    close_dialogvisible: function() {\n      this.dialogvisible = false;\n    },\n    submit_dialogvisible: function() {\n      this.dialogvisible = false;\n    },\n    cancel_dialogvisible: function() {\n      this.dialogvisible = false;\n    }\n  },\n}\n</script>\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=Ubuntu:wght@500&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Dosis:wght@200&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Quicksand:wght@700&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&display=swap');\n.font-1{font-family: 'Ubuntu', sans-serif;}\n.font-2{font-family: 'Dosis', sans-serif;}\n.font-3{font-family: 'Quicksand', sans-serif;}\n.font-4{font-family: 'Open Sans', sans-serif;}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}